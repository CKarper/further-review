{
  "name": "further-review",
  "version": "1.0.0",
  "description": "Pull request process enforcer.",
  "keyword": [
    "pull request",
    "pr",
    "glob",
    "lgtm",
    "github",
    "webhook",
    "process"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/paultyng/further-review.git"
  },
  "main": "main.js",
  "dependencies": {
    "apex.js": "^2.0.0",
    "aws-sdk": "^2.6.7",
    "axios": "^0.16.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.16.3",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-x-hub": "^1.0.4",
    "js-yaml": "^3.6.1",
    "minimatch": "^3.0.2",
    "nconf": "github:paultyng/nconf#browserify",
    "normalize-package-data": "^2.3.5"
  },
  "devDependencies": {
    "ava": "^0.18.0",
    "babel-eslint": "^7.0.0",
    "babelify": "^7.3.0",
    "browserify": "^14.2.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-module-utils": "^1.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "nyc": "^10.3.2",
    "sinon": "^2.2.0"
  },
  "scripts": {
    "start": "node ./main.js",
    "test": "eslint . && nyc ava",
    "coverage": "nyc --reporter=text-lcov ava > ./coverage/lcov.info; which lcov && lcov -l ./coverage/lcov.info",
    "test-hook": "curl -X POST -d @event.json -H 'Content-Type: application/json' http://localhost:3000/github-webhook"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
